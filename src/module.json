{
  "id": "clap",
  "name": "CLAP Host",
  "abbrev": "CL",
  "version": "0.3.4",
  "description": "Host for CLAP audio plugins",
  "author": "Move Anything",
  "license": "MIT",
  "ui": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "raw_ui": true,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": false,
    "midi_channels": 16,
    "chainable": true,
    "component_type": "sound_generator",
    "chain_params": [
      {
        "key": "plugin",
        "name": "Plugin",
        "type": "int",
        "min": 0,
        "max_param": "plugin_count",
        "default": 0
      },
      {
        "key": "preset",
        "name": "Preset",
        "type": "int",
        "min": 0,
        "max_param": "preset_count",
        "default": 0
      }
    ]
  },
  "defaults": {
    "selected_plugin": ""
  }
}
